<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="seq_appointment_booking" model="ir.sequence">
            <field name="name">Appointment Booking</field>
            <field name="code">appointment.appointment</field>
            <field name="active">TRUE</field>
            <field name="prefix">AI/</field>
            <field name="suffix">/%(y)s</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"></field>
        </record>

    </data>
</odoo>